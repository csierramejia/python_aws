// borrar contenedores
docker rm $(docker ps -qa)

// borrar imagenes
docker rmi $(docker images -a -q)

// crear imagen
docker build -t name_images .


// levantar el docker python flask
docker run -it -v /home/ubuntu/python_aws/api/app:/app --restart always --name api_flask3 -p 5000:5000 name_images

